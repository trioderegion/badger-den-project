{
  "name": "badger-den-project",
  "version": "1.0.0",
  "description": "Example project template for creating FVTT projects with Badger Den",
  "scripts": {
    "dev": "rollup -c node:rollup-config-badger-den -w --config-den src/badger-den-project:dev",
    "dev-once": "rollup -c node:rollup-config-badger-den --config-den src/badger-den-project:dev",
    "test": "rollup -c node:rollup-config-badger-den --config-den src/badger-den-project:test",
    "release": "rollup -c node:rollup-config-badger-den --config-den src/badger-den-project:release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trioderegion/badger-den-project.git"
  },
  "keywords": [
    "template",
    "fvtt",
    "foundry",
    "badger-den",
    "rollup"
  ],
  "author": "trioderegion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trioderegion/badger-den-project/issues"
  },
  "homepage": "https://github.com/trioderegion/badger-den-project#readme",
  "devDependencies": {
    "rollup-config-badger-den": "^1.7.0"
  }
}
